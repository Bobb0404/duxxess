const puzzles = {
  DS0003R: {
    size: 5,
    clues: {
      "R1C1": "W", "R1C3": "A", "R1C5": "T",
      "R3C1": "A", "R3C3": "R", "R3C5": "E",
      "R5C1": "B", "R5C3": "O", "R5C5": "X"
    }
  }
};

function loadPuzzle(puzzleId) {
  const puzzle = puzzles[puzzleId];
  if (!puzzle) return alert("Puzzle not found");

  createGrid(puzzle.size, puzzle.clues);
}
